package src1;

public class Example3 {

	public static void main(String[] args) {
		//부호 연산자 및 증감 연산자

		int a = 30;
		int b = 50;
		int c = a - b;
		c = -c;		//부호 연산자 -
		System.out.println("a= "+a);
		System.out.println("b= "+b);
		System.out.println("c= "+c);
		
		
		int sw = 1;
		System.out.println("sw= "+sw);
		sw = sw * -1;
		System.out.println("sw= "+sw);
		sw = sw * -1;
		System.out.println("sw= "+sw);
		//부호 연산자 -1
		
		
		a = 20;
		b = 20;
		
	}

}
