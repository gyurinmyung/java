package sec4;

public class Var5 {

	public static void main(String[] args) {
		
		//5번
		
		byte byteValue = 10;
		char charValue = 'A';

		int intValue = byteValue;
		int intValue1 = charValue;
		//short shortValue = charValue;
		double doubleValue = byteValue;
		
		
		// 6번
		
		int intValue2 = 10;
		char charValue1 = 'A';
		double doubleValue1 = 5.7;
	
	}

}
